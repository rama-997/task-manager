{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "resolveJsonModule": true,
    "jsx": "react",
    "paths": {
      "@libs/common": [
        "libs/common/src"
      ],
      "@libs/common/*": [
        "libs/common/src/*"
      ],
      "@libs/configs": [
        "libs/configs/src"
      ],
      "@libs/configs/*": [
        "libs/configs/src/*"
      ],
      "@libs/decorators": [
        "libs/decorators/src"
      ],
      "@libs/decorators/*": [
        "libs/decorators/src/*"
      ],
      "@libs/options": [
        "libs/options/src"
      ],
      "@libs/options/*": [
        "libs/options/src/*"
      ],
      "@libs/utils": [
        "libs/utils/src"
      ],
      "@libs/utils/*": [
        "libs/utils/src/*"
      ],
      "@src/auth": [
        "src/auth"
      ],
      "@src/auth/*": [
        "src/auth/*"
      ],
      "@src/mail": [
        "src/mail"
      ],
      "@src/mail/*": [
        "src/mail/*"
      ],
      "@src/role": [
        "src/role"
      ],
      "@src/role/*": [
        "src/role/*"
      ],
      "@src/token": [
        "src/token"
      ],
      "@src/token/*": [
        "src/token/*"
      ]
    }
  }
}