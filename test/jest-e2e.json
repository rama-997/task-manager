{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@libs/configs": "<rootDir>/../libs/configs/src",
    "@libs/configs/(.*)": "<rootDir>/../libs/configs/src/$1",
    "@libs/options": "<rootDir>/../libs/options/src",
    "@libs/options/(.*)": "<rootDir>/../libs/options/src/$1"
  }
}